plotdata = function(ymin=0, ymax=1000, filename="Output1.csv", directory=workingdirectory) {
datatoplot = read.table(paste(directory, filename, sep=""), header = TRUE, sep = ",")
mycolor = colors()[73]
plot(
Generation~BeakSize,
main = filename,
pch = 20,
col = mycolor, cex=0.4,
xlim = c(0,11),
ylim = c(ymin,ymax),
data = datatoplot
)
}
